<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <RootNamespace>OrbintSoft.Yauaa.Commandline</RootNamespace>
    <Version>5.5-beta.2</Version>
	<Authors>Stefano Balzarotti, Niels Basjes</Authors>												   
    <AssemblyVersion>5.5.1.2</AssemblyVersion>
	<Copyright>Copyright (C) 2013-2018 Niels Basjes, Porting by OrbintSoft</Copyright>				 
    <FileVersion>5.5.1.2</FileVersion>
	<SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\orbintsoft-public.snk</AssemblyOriginatorKeyFile>
    <Company>OrbintSoft</Company>
    <Description>A simple commandline application to parse the user agent</Description>
    <PackageLicenseUrl>https://github.com/OrbintSoft/yauaa.netstandard/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/OrbintSoft/yauaa.netstandard</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <NeutralLanguage>en</NeutralLanguage>
    <ApplicationIcon>icon.ico</ApplicationIcon>
    <PackageIconUrl>https://raw.githubusercontent.com/OrbintSoft/yauaa.netstandard/master/assets/icon.png</PackageIconUrl>																				  
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.3.0" />
    <PackageReference Include="OrbintSoft.Yauaa.NetStandard" Version="5.5.0-beta.1">
		<IncludeAssets>all</IncludeAssets>
	</PackageReference>
  </ItemGroup>

  <ItemGroup>
    <None Update="log4net.config">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
